{
    "name": "luqta/localization",
    "description": "Localization middleware package",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdullah Abdelqader",
            "email": "3bdullahg97@gmail.com",
            "homepage": "https://twitter.com/3bdullahg97"
        }
    ],
    "homepage": "https://github.com/luqta/localization",
    "keywords": ["Laravel", "Localization"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luqta\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luqta\\Localization\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luqta\\Localization\\LocalizationServiceProvider"
            ],
            "aliases": {
                "Localization": "Luqta\\Localization\\Facades\\Localization"
            }
        }
    }
}
